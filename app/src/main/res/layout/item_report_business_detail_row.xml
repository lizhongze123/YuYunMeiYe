<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl"
    android:layout_width="match_parent"
    android:layout_height="@dimen/y100"
    android:background="@color/white"
    android:gravity="center">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_storeName"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="门店"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_consumeType"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="消费类型"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_billNum"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="单据号"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_memberName"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="会员姓名"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_cardNum"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="卡号"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_service"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="项目"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_package"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="套餐"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_product"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="产品"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_one"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="成交金额"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_two"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="现金"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_three"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="刷卡"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_four"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="转账"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_five"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="微信"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_six"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="支付宝"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_seven"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="储值"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_eight"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="积分余额"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_nine"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="免单"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_ten"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="现金券"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_eleven"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="欠款"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

        <TextView
            android:id="@+id/tv_twelve"
            android:layout_width="@dimen/x180"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="欠款"
            android:textColor="#274335"
            android:textSize="@dimen/x20" />

    </LinearLayout>

</RelativeLayout>